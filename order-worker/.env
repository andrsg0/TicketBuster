# TicketBuster Order Worker - Environment Configuration
# Local development settings

# Database Configuration (PostgreSQL on Docker port 5432)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ticketbuster
DB_USER=admin
DB_PASSWORD=admin
DB_SCHEMA=db_orders

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/

# Queue Names
ORDERS_QUEUE=orders_queue
NOTIFICATIONS_QUEUE=notifications_queue

# gRPC Configuration (Catalog Service)
GRPC_CATALOG_HOST=localhost
GRPC_CATALOG_PORT=50051

# Worker Configuration
PREFETCH_COUNT=1
WORKER_NAME=order-worker-local

# Logging
LOG_LEVEL=INFO
